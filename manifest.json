{
    "name": "MultiLinkOpen",
    "description": "Chrome App for opening links by keyword",
    "version": "0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "calculator-16.png",
        "default_title": "Multi Link Open",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "activeTab"
    ],
    "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
    "icons": {
        "16": "calculator-16.png",
        "128": "calculator-128.png"
    }
}